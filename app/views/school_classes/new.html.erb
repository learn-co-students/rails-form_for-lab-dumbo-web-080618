<h1>School Class Form</h1>
<%= form_for(SchoolClass.new) do |f| %>
  <label for="school_class_title">School Class Title:</label>
  <%= f.text_field :title %><br>
  
  <label for="school_class_room_number">School Class Room Number:</label>
  <%= f.text_field :room_number %><br>
  <%= f.submit "Create School class"%>
<% end %>