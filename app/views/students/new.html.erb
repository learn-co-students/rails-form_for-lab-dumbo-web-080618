<h1>Student Form</h1>

<%= form_for school_classes_path do |f| %>
  <%= f.label :first_name %>
  <%= text_field_tag :first_name, params[:first_name], :id => 'student_first_name' %>

  <%= f.label :last_name %>
  <%= text_field_tag :last_name, params[:last_name], :id => 'student_last_name' %>
  <%= f.submit 'Create Student' %>
<% end %>
